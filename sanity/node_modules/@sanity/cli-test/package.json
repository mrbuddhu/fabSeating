{
  "name": "@sanity/cli-test",
  "version": "0.0.2-alpha.1",
  "description": "Sanity CLI test helpers and utilities",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "cli",
    "tool"
  ],
  "homepage": "https://github.com/sanity-io/cli",
  "bugs": "https://github.com/sanity-io/cli/issues",
  "repository": "sanity-io/cli",
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@oclif/core": "^4.5.3",
    "ansis": "^3.17.0",
    "nock": "^14.0.5"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.14.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.19.24",
    "eslint": "^9.39.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "@repo/eslint-config": "0.0.0",
    "@repo/tsconfig": "3.70.0"
  },
  "engines": {
    "node": ">=20.19.1 <22 || >=22.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc --delete-dir-on-start --strip-leading-paths --out-dir dist/ src",
    "build:types": "tsc --project tsconfig.lib.json",
    "check:types": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest run",
    "posttest": "pnpm run lint",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "swc --delete-dir-on-start --strip-leading-paths --out-dir dist/ --watch src"
  }
}